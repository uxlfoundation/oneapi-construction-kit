  inline float __abacus_infinity() {
    union Both {
      float f;
      int i;
    } both;

    both.i = 0x7f800000;
    return both.f;
  }

  inline float __abacus_nan() {
    union Both {
      float f;
      int i;
    } both;

    both.i = 0x7f800001;
    return both.f;
  }

