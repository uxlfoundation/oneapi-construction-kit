; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_rotate_v3i32 "u_rotate_v3i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
             %v3i32_t = OpTypeVector %i32_t 3
       %u_rotate_fn_t = OpTypeFunction %void_t
               %i32_2 = OpConstant %i32_t 2
               %i32_4 = OpConstant %i32_t 4
             %v3i32_2 = OpConstantComposite %v3i32_t %i32_2 %i32_2 %i32_2
             %v3i32_4 = OpConstantComposite %v3i32_t %i32_4 %i32_4 %i32_4

; All functions
      %u_rotate_v3i32 = OpFunction %void_t None %u_rotate_fn_t
; CHECK: define spir_kernel void @u_rotate_v3i32()
                   %2 = OpLabel
                   %3 = OpExtInst %v3i32_t %1 rotate %v3i32_2 %v3i32_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x i32> @_Z6rotateDv3_jS_(<3 x i32> <i32 2, i32 2, i32 2>, <3 x i32> <i32 4, i32 4, i32 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
