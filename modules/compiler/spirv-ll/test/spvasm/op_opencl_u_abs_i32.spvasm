; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_abs_i32 "u_abs_i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %u32_t = OpTypeInt 32 0
               %i32_t = OpTypeInt 32 1
          %u_abs_fn_t = OpTypeFunction %void_t
               %u32_2 = OpConstant %u32_t 2
               %i32_2 = OpConstant %u32_t 2

; All functions
           %u_abs_i32 = OpFunction %void_t None %u_abs_fn_t
; CHECK: define spir_kernel void @u_abs_i32()
                   %2 = OpLabel
                   %3 = OpExtInst %u32_t %1 u_abs %u32_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func i32 @_Z3absj(i32 2)
                   %4 = OpExtInst %u32_t %1 u_abs %i32_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func i32 @_Z3absj(i32 2)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
