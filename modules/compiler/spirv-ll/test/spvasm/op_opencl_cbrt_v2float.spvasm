; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"                        
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %cbrt_v2float "cbrt_v2float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v2float_t = OpTypeVector %float_t 2
           %cbrt_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
           %v2float_1 = OpConstantComposite %v2float_t %float_1 %float_1

; All functions
        %cbrt_v2float = OpFunction %void_t None %cbrt_fn_t
; CHECK: define spir_kernel void @cbrt_v2float()
                   %2 = OpLabel
                   %3 = OpExtInst %v2float_t %1 cbrt %v2float_1
; CHECK: = call spir_func <2 x float> @_Z4cbrtDv2_f(<2 x float> {{<(float 1.000000e\+00(, )?)+>|splat \(float 1.000000e\+00\)}})
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
