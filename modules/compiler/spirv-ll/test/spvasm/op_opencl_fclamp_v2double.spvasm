; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 -c Float64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fclamp_v2double "fclamp_v2double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
          %v2double_t = OpTypeVector %double_t 2
         %fclamp_fn_t = OpTypeFunction %void_t
            %double_0 = OpConstant %double_t 0.8
            %double_1 = OpConstant %double_t 1.0
            %double_2 = OpConstant %double_t 2.0
          %v2double_0 = OpConstantComposite %v2double_t %double_0 %double_0
          %v2double_1 = OpConstantComposite %v2double_t %double_1 %double_1
          %v2double_2 = OpConstantComposite %v2double_t %double_2 %double_2

; All functions
     %fclamp_v2double = OpFunction %void_t None %fclamp_fn_t
; CHECK: define spir_kernel void @fclamp_v2double()
                   %2 = OpLabel
                   %3 = OpExtInst %v2double_t %1 fclamp %v2double_0 %v2double_1 %v2double_2
; CHECK: {{%.*}} = call spir_func <2 x double> @_Z5clampDv2_dS_S_(<2 x double> <double 8.000000e-01, double 8.000000e-01>, <2 x double> <double 1.000000e+00, double 1.000000e+00>, <2 x double> <double 2.000000e+00, double 2.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
