; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"                        
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %atanpi_v3float "atanpi_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
         %atanpi_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
      %atanpi_v3float = OpFunction %void_t None %atanpi_fn_t
; CHECK: define spir_kernel void @atanpi_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 atanpi %v3float_1
; CHECK: {{%.*}} = call spir_func <3 x float> @_Z6atanpiDv3_f(<3 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
