; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_max_v4i64 "u_max_v4i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
             %v4i64_t = OpTypeVector %i64_t 4
          %u_max_fn_t = OpTypeFunction %void_t
               %i64_1 = OpConstant %i64_t 1
               %i64_2 = OpConstant %i64_t 2
             %v4i64_1 = OpConstantComposite %v4i64_t %i64_1 %i64_1 %i64_1 %i64_1
             %v4i64_2 = OpConstantComposite %v4i64_t %i64_2 %i64_2 %i64_2 %i64_2

; All functions
         %u_max_v4i64 = OpFunction %void_t None %u_max_fn_t
; CHECK: define spir_kernel void @u_max_v4i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v4i64_t %1 u_max %v4i64_1 %v4i64_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x i64> @_Z3maxDv4_mS_(<4 x i64> <i64 1, i64 1, i64 1, i64 1>, <4 x i64> <i64 2, i64 2, i64 2, i64 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
