; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %spv_file_s | %filecheck %s
                          OpCapability Kernel
                          OpCapability Addresses
                          OpCapability Float64
                     %1 = OpExtInstImport "OpenCL.std"
                          OpMemoryModel Physical64 OpenCL
                          OpEntryPoint Kernel %fast_distance_v3double "fast_distance_v3double"
                          OpSource OpenCL_C 102000

; All types, variables and constants
                %void_t = OpTypeVoid
              %double_t = OpTypeFloat 64
            %v3double_t = OpTypeVector %double_t 3
    %fast_distance_fn_t = OpTypeFunction %void_t
              %double_2 = OpConstant %double_t 2.0
              %double_4 = OpConstant %double_t 4.0
            %v3double_2 = OpConstantComposite %v3double_t %double_2 %double_2 %double_2
            %v3double_4 = OpConstantComposite %v3double_t %double_4 %double_4 %double_4

; All functions
%fast_distance_v3double = OpFunction %void_t None %fast_distance_fn_t
; CHECK: define spir_kernel void @fast_distance_v3double()
                     %2 = OpLabel
                     %3 = OpExtInst %double_t %1 fast_distance %v3double_2 %v3double_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func double @_Z13fast_distanceDv3_dS_(<3 x double> <double 2.000000e+00, double 2.000000e+00, double 2.000000e+00>, <3 x double> <double 4.000000e+00, double 4.000000e+00, double 4.000000e+00>)
                          OpReturn
; CHECK: ret void
                          OpFunctionEnd
; CHECK: }
