; RUN: %spirv-ll -a OpenCL -b 32 -c Addresses -c Float64 %p/op_opencl_vloadn_double_32bit.spv | %filecheck %s
               OpCapability Addresses
               OpCapability Kernel
               OpCapability Vector16
               OpCapability Float64
          %1 = OpExtInstImport "OpenCL.std"
               OpMemoryModel Physical32 OpenCL
               OpEntryPoint Kernel %7 "foo"
               OpSource OpenCL_C 102000
               OpName %in "in"
               OpName %loc "loc"
               OpName %entry "entry"
               OpName %call "call"
               OpName %call1 "call1"
               OpName %call2 "call2"
               OpName %call3 "call3"
               OpName %call4 "call4"
               OpName %call5 "call5"
               OpName %call6 "call6"
               OpName %call7 "call7"
               OpName %call8 "call8"
               OpName %call9 "call9"
       %uint = OpTypeInt 32 0
     %uint_0 = OpConstant %uint 0
       %void = OpTypeVoid
     %double = OpTypeFloat 64
%_ptr_CrossWorkgroup_double = OpTypePointer CrossWorkgroup %double
%_ptr_Workgroup_double = OpTypePointer Workgroup %double
          %6 = OpTypeFunction %void %_ptr_CrossWorkgroup_double %_ptr_Workgroup_double
   %v2double = OpTypeVector %double 2
   %v3double = OpTypeVector %double 3
   %v4double = OpTypeVector %double 4
   %v8double = OpTypeVector %double 8
  %v16double = OpTypeVector %double 16
          %7 = OpFunction %void None %6
         %in = OpFunctionParameter %_ptr_CrossWorkgroup_double
        %loc = OpFunctionParameter %_ptr_Workgroup_double
      %entry = OpLabel
       %call = OpExtInst %v2double %1 vloadn %uint_0 %in 2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x double> @_Z6vload2jPU3AS1Kd(i32 0, ptr addrspace(1) %in)
      %call1 = OpExtInst %v3double %1 vloadn %uint_0 %in 3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x double> @_Z6vload3jPU3AS1Kd(i32 0, ptr addrspace(1) %in)
      %call2 = OpExtInst %v4double %1 vloadn %uint_0 %in 4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x double> @_Z6vload4jPU3AS1Kd(i32 0, ptr addrspace(1) %in)
      %call3 = OpExtInst %v8double %1 vloadn %uint_0 %in 8
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x double> @_Z6vload8jPU3AS1Kd(i32 0, ptr addrspace(1) %in)
      %call4 = OpExtInst %v16double %1 vloadn %uint_0 %in 16
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x double> @_Z7vload16jPU3AS1Kd(i32 0, ptr addrspace(1) %in)
      %call5 = OpExtInst %v2double %1 vloadn %uint_0 %loc 2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x double> @_Z6vload2jPU3AS3Kd(i32 0, ptr addrspace(3) %loc)
      %call6 = OpExtInst %v3double %1 vloadn %uint_0 %loc 3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x double> @_Z6vload3jPU3AS3Kd(i32 0, ptr addrspace(3) %loc)
      %call7 = OpExtInst %v4double %1 vloadn %uint_0 %loc 4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x double> @_Z6vload4jPU3AS3Kd(i32 0, ptr addrspace(3) %loc)
      %call8 = OpExtInst %v8double %1 vloadn %uint_0 %loc 8
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x double> @_Z6vload8jPU3AS3Kd(i32 0, ptr addrspace(3) %loc)
      %call9 = OpExtInst %v16double %1 vloadn %uint_0 %loc 16
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x double> @_Z7vload16jPU3AS3Kd(i32 0, ptr addrspace(3) %loc)
               OpReturn
               OpFunctionEnd
