; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                            OpCapability Kernel
                            OpCapability Addresses
                            OpCapability Vector16
                       %1 = OpExtInstImport "OpenCL.std"
                            OpMemoryModel Physical64 OpenCL
                            OpEntryPoint Kernel %native_divide_v16float "native_divide_v16float"
                            OpSource OpenCL_C 102000

; All types, variables and constants
                  %void_t = OpTypeVoid
                 %float_t = OpTypeFloat 32
              %v16float_t = OpTypeVector %float_t 16
      %native_divide_fn_t = OpTypeFunction %void_t
                 %float_7 = OpConstant %float_t 7.0
                %float_22 = OpConstant %float_t 22.0
              %v16float_7 = OpConstantComposite %v16float_t %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7 %float_7
             %v16float_22 = OpConstantComposite %v16float_t %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22 %float_22

; All functions
  %native_divide_v16float = OpFunction %void_t None %native_divide_fn_t
; CHECK: define spir_kernel void @native_divide_v16float()
                       %2 = OpLabel
                       %3 = OpExtInst %v16float_t %1 native_divide %v16float_22 %v16float_7
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x float> @_Z13native_divideDv16_fS_(<16 x float> <float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01>, <16 x float> <float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00>)
                            OpReturn
; CHECK: ret void
                            OpFunctionEnd
; CHECK: }
