; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %tanh_v3float "tanh_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
           %tanh_fn_t = OpTypeFunction %void_t
             %float_0 = OpConstant %float_t 0.79
           %v3float_0 = OpConstantComposite %v3float_t %float_0 %float_0 %float_0

; All functions
        %tanh_v3float = OpFunction %void_t None %tanh_fn_t
; CHECK: define spir_kernel void @tanh_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 tanh %v3float_0
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z4tanhDv3_f(<3 x float> <float 0x3FE947AE20000000, float 0x3FE947AE20000000, float 0x3FE947AE20000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
