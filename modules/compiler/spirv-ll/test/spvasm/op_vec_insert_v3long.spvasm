; RUN: spirv-ll-tool -a Vulkan -c Float64 -c Int64 %spv_file_s | FileCheck %s
            OpCapability Shader
            OpCapability Float64
            OpCapability Int64
       %1 = OpExtInstImport "GLSL.std.450"
            OpMemoryModel Logical GLSL450
            OpEntryPoint GLCompute %main "main"
            OpExecutionMode %main LocalSize 1 1 1
            OpSource GLSL 450
            OpName %main "main"
            OpName %a "a"
               OpName %res "res"
       %void = OpTypeVoid
          %2 = OpTypeFunction %void
        %long = OpTypeInt 64 1
     %idx_ty = OpTypeInt 32 0
         %v3 = OpTypeVector %long 3
     %ptr_v3 = OpTypePointer Function %v3
          %3 = OpConstant %long -4200000000
          %4 = OpConstant %long -4200000000
          %5 = OpConstant %long -4200000000
          %6 = OpConstant %idx_ty 1
          %7 = OpConstantComposite %v3 %3 %4 %5
       %main = OpFunction %void None %2
          %9 = OpLabel
          %a = OpVariable %ptr_v3 Function
        %res = OpVariable %ptr_v3 Function
               OpStore %a %7
         %10 = OpLoad %v3 %a
         %11 = OpVectorInsertDynamic %v3 %10 %5 %6
               OpStore %res %11
               OpReturn
               OpFunctionEnd
; CHECK: ; ModuleID = '{{.*}}'
; CHECK: define spir_kernel void @main()
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = alloca <3 x i64>
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = alloca <3 x i64>
; CHECK: store <3 x i64> <i64 -4200000000, i64 -4200000000, i64 -4200000000>, ptr {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = load <3 x i64>, ptr {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = insertelement <3 x i64> {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}, i64 -4200000000, i32 1
; CHECK: store <3 x i64> {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}, ptr {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}
; CHECK: ret void
