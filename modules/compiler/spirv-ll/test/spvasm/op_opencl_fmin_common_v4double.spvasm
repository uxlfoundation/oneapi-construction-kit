; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 -c Float64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fmin_common_v4double "fmin_common_v4double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
          %v4double_t = OpTypeVector %double_t 4
    %fmin_common_fn_t = OpTypeFunction %void_t
            %double_1 = OpConstant %double_t 1.9
            %double_2 = OpConstant %double_t 2.4
          %v4double_1 = OpConstantComposite %v4double_t %double_1 %double_1 %double_1 %double_1
          %v4double_2 = OpConstantComposite %v4double_t %double_2 %double_2 %double_2 %double_2

; All functions
%fmin_common_v4double = OpFunction %void_t None %fmin_common_fn_t
; CHECK: define spir_kernel void @fmin_common_v4double()
                   %2 = OpLabel
                   %3 = OpExtInst %v4double_t %1 fmin_common %v4double_1 %v4double_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x double> @_Z3minDv4_dS_(<4 x double> <double 1.900000e+00, double 1.900000e+00, double 1.900000e+00, double 1.900000e+00>, <4 x double> <double 2.400000e+00, double 2.400000e+00, double 2.400000e+00, double 2.400000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
